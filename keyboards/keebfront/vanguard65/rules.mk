# Processor frequency
F_CPU = 8000000
# Rules.mk for Vanguard65 keyboard

# Enable basic features
MIDI_ENABLE = yes          # Enable MIDI functionality
RGBLIGHT_ENABLE = yes      # Enable RGB lighting
NKRO_ENABLE = yes          # Enable N-Key Rollover
BOOTMAGIC_ENABLE = yes     # Enable Bootmagic key combinations
TAP_DANCE_ENABLE = yes     # Enable tap dance functionality

# Optional features
SLEEP_LED_ENABLE = no      # Disable sleep LED functionality
BACKLIGHT_ENABLE = no      # Disable backlight functionality (use RGBLIGHT instead)
